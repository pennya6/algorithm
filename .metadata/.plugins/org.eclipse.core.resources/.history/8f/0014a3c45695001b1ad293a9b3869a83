package work1;

public class quickSort {
	public static int partition(int low, int high) {
		int i=low;
		int j=high+1;
		int a[]=new int[]{};
		while(true) {
			while(a[++i]<a[low])
				if(i==high) break;
			while(a[low]<a[--j])
				if(i==low) break;
			if(i>=j) break;
			
		}
		
		return j;
	}
	public static void quickSort() {
		
	}

}
