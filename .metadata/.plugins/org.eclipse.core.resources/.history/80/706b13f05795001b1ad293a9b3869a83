package work1;

public class quickSort {
	public static int partition(Object []a,int low, int high) {
		int i=low;
		int j=high+1;
		Object x=a[low];
		while(true) {
			while(a[++i]<x)
				if(i==high) break;
			while(x<a[--j])
				if(i==low) break;
			if(i>=j) break;
			int t=0;
			a[i]=t;
			a[i]=a[j];
			a[j]=t;
		}
		return j;
	}
	public static void quickSort(Object []a,int low, int high,Comparator comparator) {
		if(low>=high) return ;
		int pivot;
		pivot=partition(a,low,high);
		quickSort(low,pivot-1);
		quickSort(pivot+1,high);
	}

}
