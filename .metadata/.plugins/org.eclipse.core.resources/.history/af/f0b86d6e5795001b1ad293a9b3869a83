package work1;

public class quickSort {
	public static int partition(int low, int high) {
		int i=low;
		int j=high+1;
		int a[]=new int[]{};
		while(true) {
			while(a[++i]<a[low])
				if(i==high) break;
			while(a[low]<a[--j])
				if(i==low) break;
			if(i>=j) break;
			int t=0;
			a[i]=t;
			a[i]=a[j];
			a[j]=t;
		}
		return j;
	}
	public static void quickSort(int low, int high) {
		if(low>=high) return ;
		int pivot;
		pivot=partition(low,high);
		quickSort(low,pivot-1);
		quickSort(pivot+1,high);
	}

}
